        <vbox text="Page 1" width="100%">
            <button text="Button 1" width="100%" height="20%" styleName="red" />
            <button text="Button 2" width="100%" height="20%" styleName="green" />
            <button text="Button 3" width="100%" height="20%" styleName="yellow" />
            <button text="Button 4" width="100%" height="20%" styleName="grey" />
            <button text="Button 5" width="100%" height="20%" />
        </vbox>   
        <vbox text="Page 2" width="100%">
            <listview width="100%" height="100%">
                <data>
                    <item text="Item 1" />
